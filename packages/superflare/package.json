{
  "name": "superflare",
  "version": "0.1.1",
  "description": "A full-stack framework for Cloudflare Workers.",
  "main": "dist/index.js",
  "types": "dist/index.types.d.ts",
  "bugs": {
    "url": "https://github.com/jplhomer/superflare/issues"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup --format esm,cjs",
    "dev": "tsup --format esm,cjs --watch",
    "prepack": "pnpm build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "superflare": "bin/superflare.js"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "framework",
    "full-stack",
    "orm",
    "d1"
  ],
  "author": "Josh Larson <jplhomer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@web3-storage/multipart-parser": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.4",
    "confbox": "^0.1.3",
    "esbuild": "^0.20.2",
    "esbuild-register": "^3.5.0",
    "gunzip-maybe": "^1.4.2",
    "npx-import": "^1.1.4",
    "pluralize": "^8.0.0",
    "tar-fs": "^3.0.5",
    "tiny-invariant": "^1.3.3",
    "wrangler": "^3.41.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240329.0",
    "@miniflare/core": "^2.14.2",
    "@miniflare/d1": "^2.14.2",
    "@miniflare/shared": "^2.14.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.9",
    "@types/gunzip-maybe": "^1.4.2",
    "@types/node": "^20.12.2",
    "@types/pluralize": "^0.0.33",
    "@types/tar-fs": "^2.0.4",
    "@types/yargs": "^17.0.32",
    "better-sqlite3": "^9.4.3",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  }
}
