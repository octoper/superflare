{
  "name": "remix-cms",
  "version": "1.0.0",
  "description": "An example Superflare app powered by Remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:remix",
    "build:remix": "remix build",
    "dev:remix": "remix watch",
    "dev:superflare": "NODE_ENV=development superflare dev -b APP_KEY=secret",
    "dev": "pnpm build && concurrently \"npm:dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:superflare",
    "migrate": "superflare migrate"
  },
  "keywords": [],
  "author": "Josh Larson <jplhomer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.1",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.3",
    "@remix-run/cloudflare": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@superflare/remix": "workspace:*",
    "@tailwindcss/forms": "^0.5.7",
    "clsx": "^2.1.0",
    "highlight.js": "^11.9.0",
    "isbot": "^5.1.3",
    "marked": "^12.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superflare": "workspace:*",
    "textarea-markdown-editor": "^1.0.4",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240329.0",
    "@faker-js/faker": "^8.4.1",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@remix-run/server-runtime": "^2.8.1",
    "@tailwindcss/typography": "^0.5.12",
    "@types/marked": "^6.0.0",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsconfig": "workspace:*",
    "typescript": "^5.4.3",
    "wrangler": "^3.41.0"
  },
  "engines": {
    "node": ">=16.13"
  }
